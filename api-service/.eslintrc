{
    "extends": [
      "prettier",
      "eslint:recommended",
      "plugin:node/recommended",
      "plugin:jest/recommended",
      "plugin:prettier/recommended"
    ],
    "env": {
      "node": true,
      "jest/globals": true
    },
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 2021
    },
    "rules": {
      "no-process-exit": "off",
      "no-unused-vars": "off",
      "node/no-unpublished-require": "off"
    }
  }
